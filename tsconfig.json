{
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"lib": ["esnext"],
		"types": [
			"@cloudflare/workers-types",
			"@types/range-parser"
		],
		"noEmit": true,
		"moduleResolution": "node",
		"isolatedModules": true,
